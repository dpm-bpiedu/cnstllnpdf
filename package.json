{
  "name": "cnstllnpdf",
  "version": "1.0.0",
  "description": "change html to pdf files",
  "main": "index.js",
  "scripts": {
    "start": "cd app && yarn start",
    "build": "cd app && yarn build",
    "watch-sass": "node-sass app/scss/style.scss app/src/index.css --watch",
    "compile-sass": "node-sass app/scss/style.scss app/scss/style.comp.css",
    "prefix-css": "postcss --use autoprefixer -b 'last 5 versions' app/scss/style.comp.css -o app/scss/style.prefix.css",
    "compress-css": "node-sass app/scss/style.prefix.css app/src/index.css --output-style compressed",
    "build-css": "npm-run-all compile-sass prefix-css compress-css"
  },
  "repository": "git@github.com:dpm-bpiedu/cnstllnpdf.git",
  "author": "Dan Moneypenny <catullus1973@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^9.6.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
